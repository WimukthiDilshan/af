{
  "version": 2,
  "builds": [
    {
      "src": "my-project/package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "my-project/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "my-project/dist/$1"
    }
  ]
} 